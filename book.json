{
    "plugins": [
        "expandable-chapters",
        "splitter",
        "prism",
  		  "-highlight",
        "back-to-top-button",
        "page-toc-button",
        "-lunr",
        "-search",
        "search-plus",
        "copy-code-button",
        "alerts",
        "lightbox",
        "katex",
        "mathjax-full",
        "url-embed",
        "embed-pdf",
        "codeblock-label",
        "github-embed",
        "include-codeblock"
    ],
    "pluginsConfig": {
  "prism": {
    "css": [
      "prismjs/themes/prism-okaidia.css"
    ],
    "lang": {
      "shell": "bash"
    }
  },
    "include-codeblock": {
    "template": "acefull",
    "unindent": true,
    "edit": true,
    "theme": "tomorrow_night_eighties"
    },
   "github-embed": {
    "showLink": true,
    "urls": [
        {
            "regex": "https://github.com/peterlulu666/CProgrammingTutorial"
        }
    ]
   }
}

}

